{
    "name": "my-apollo-server-express-starter",
    "version": "1.1.0",
    "description": "",
    "main": "src/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "prettier": "prettier --write .",
        "lint": "eslint src/**/*.{ts,tsx}",
        "format": "eslint src/**/*.ts --fix",
        "build": "tsc -p . && yarn copy-files",
        "copy-files": "shopt -s globstar && cp -r src/**/!(*.ts) dist/",
        "dev": "nodemon --ext ts,tsx,graphql --watch src --exec ts-node -r tsconfig-paths/register src/index.ts"
    },
    "keywords": [
        "graphql",
        "apollo",
        "typescript",
        "express",
        "mongoose"
    ],
    "author": "John Olatubosun <toluolatubosun@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@apollo/server": "^4.5.0",
        "@aws-sdk/client-s3": "^3.431.0",
        "@aws-sdk/client-secrets-manager": "^3.431.0",
        "@aws-sdk/client-ses": "^3.414.0",
        "@aws-sdk/s3-request-presigner": "^3.431.0",
        "@react-email/components": "^0.0.7",
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.32.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "graphql": "^16.6.0",
        "graphql-depth-limit": "^1.1.0",
        "helmet": "^5.1.1",
        "joi": "^17.9.2",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.6.0",
        "morgan": "^1.10.0",
        "ms": "^2.1.3",
        "nanoid": "3.3.4",
        "nodemailer": "^6.7.8",
        "react": "^18.2.0",
        "streamifier": "^0.1.1"
    },
    "devDependencies": {
        "@aws-sdk/types": "^3.433.0",
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.5",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.13",
        "@types/graphql-depth-limit": "^1.1.3",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/morgan": "^1.9.3",
        "@types/ms": "^0.7.31",
        "@types/node": "^18.7.8",
        "@types/nodemailer": "^6.4.5",
        "@types/react": "^18.2.31",
        "@types/streamifier": "^0.1.1",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "eslint": "^8.23.1",
        "nodemon": "^3.0.1",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^4.7.4"
    }
}
